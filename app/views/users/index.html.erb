<% provide(:title, 'All users') %>
<section class="text-center">
<h1 class="h1 margintop blue">ユーザー</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li class="list-between">
      <%= gravatar_for user%>
      <%= link_to user.name, user , class:"listname"%>
    </li>
  <% end %>
</ul>
</section>